 column=(type=char(30) updatewhereclause=yes name=ajustes_usuarios_codigo dbname="AJUSTES.USUARIOS_CODIGO" )
 column=(type=datetime updatewhereclause=yes name=ajustes_ajustes_fecha dbname="AJUSTES.AJUSTES_FECHA" )
 column=(type=long updatewhereclause=yes name=ajustes_ajustes_cantidad dbname="AJUSTES.AJUSTES_CANTIDAD" )

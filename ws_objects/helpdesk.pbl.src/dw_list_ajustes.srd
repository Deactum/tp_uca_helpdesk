 retrieve="declare @pagina int = :pagina
declare @registros int = :registros
declare @saltar int = @pagina*@registros
SELECT
A.COMPONENTES_CODIGO,
A.USUARIOS_CODIGO,
A.AJUSTES_FECHA,
A.AJUSTES_CANTIDAD,
U.USUARIOS_NOMBRE +' '+ U.USUARIOS_APELLIDO AS USUARIO,
c.COMPONENTES_NOMBRE
FROM AJUSTES A
JOIN USUARIOS U ON A.USUARIOS_CODIGO = U.USUARIOS_CODIGO
JOIN COMPONENTES C ON A.COMPONENTES_CODIGO = C.COMPONENTES_CODIGO
where a.USUARIOS_CODIGO like :busqueda
or u.USUARIOS_NOMBRE like :busqueda
or u.USUARIOS_APELLIDO like :busqueda
or a.AJUSTES_FECHA like :busqueda
order by 1 desc
offset @saltar rows
fetch next @registros rows only" arguments=(("pagina", number),("registros", number),("busqueda", string)) )
